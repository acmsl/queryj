  <build>
    <testSourceDirectory>src/test/java</testSourceDirectory>
    <plugins>
<!--
       <plugin>
        <groupId>uk.co.badgersinfoil.maven.plugins</groupId>
        <artifactId>maven-antlr3-plugin</artifactId>
        <executions>
          <execution>
            <goals>
