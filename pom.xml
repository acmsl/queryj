      <id>acmsl-repository</id>
      <name>ACM-SL Repository</name>
      <url>scp://maven.acm-sl.org/lib</url>
      <layout>legacy</layout>
    </repository>
    <site>
      <id>maven.acm-sl.org</id>
      <url>scp://maven.acm-sl.org/</url>
    </site>
  </distributionManagement>
  <reporting>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-project-info-reports-plugin</artifactId>
      </plugin>
    </plugins>
  </reporting>
